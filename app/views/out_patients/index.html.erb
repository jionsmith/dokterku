<div class="row">
<div class="col-xs-12 table-responsive">
  <% if @out_patients.any? %>
    <table class="table table-stripped" id="out_patients_table">
      <thead>
        <tr>
          <th>RM Number</th>
          <th>Queue Number</th>
          <th>Full Name</th>
          <th>Doctor</th>
          <th>Polyclinic</th>
          <th>Waiting Time</th>
          <% if is_reg_admin? %><th>Actions</td><% end %>
        </tr>
      </thead>
      <tbody>
        <script>
        var timespan = [];
        var array = [];
        </script>
        <% @out_patients.each do |out_patient| %>
          <tr>
            <td><%= out_patient.rm_number %></td>
            <td><%= out_patient.queue_number %></td>
            <td><%= out_patient.patient_name if is_reg_admin? || is_doctor? %></td>
            <td><%= out_patient.doctor_name %></td>
            <td><%= out_patient.polyclinic_name %></td>
            <td><%= out_patient.created_at %></td>
            <% if is_reg_admin? %><td>
              <%= link_to 'Details', out_patient_path(out_patient), class: 'btn btn-info' %>
              <%= link_to 'Edit', edit_out_patient_path(out_patient), class: 'btn btn-primary' %>
              <%= link_to 'Delete', out_patient_path(out_patient), method: :delete, class: 'btn btn-danger', data: { confirm: 'Are you sure?' } %>
            </td><% end %>
          </tr>
          <script>
          array.push("<%= out_patient.created_at %>");
          </script>
        <% end %>
        <script>
  function msToTime(s) {
  var ms = s % 1000;
  s = (s - ms) / 1000;
  var secs = s % 60;
  s = (s - secs) / 60;
  var mins = s % 60;
  var hrs = (s - mins) / 60;

  return hrs + ' Hours ' + mins + ' Minutes ' + secs + ' Seconds';
}
$(document).on('turbolinks:load', function(){
    for (var i = 0; i < array.length; i++) {
      (function(i){
        if (i > 0) {
          var t = Date.parse(array[i]) - Date.parse(array[i - 1]);
          timespan.push(setInterval(function(){
            $('#out_patients_table tbody tr:nth-child(' + (i + 1) + ') td:nth-child(6)').text(msToTime(t));
            t -= 1000;
            if (t <= 0) {
              clearInterval(timespan);
            }
          }, 1000));
        } else {
          $('#out_patients_table tbody tr:nth-child(1) td:nth-child(6)').text('0 Hours 0 Minutes 0 Seconds');
        }
      })(i);
    }
  });
        </script>
      </tbody>
    </table>
  <% else %>
    <div class="alert alert-info">You don't have any Out Patients right now.</div>
  <% end %>
  </div>
</div>
</div>
                </div>



        </div>
    </div>
